    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.4",
    "browser-run": "^4.0.2",
    "browserify": "^14.4.0",
    "css-loader": "^0.28.4",
    "csscomb": "^4.1.0",
